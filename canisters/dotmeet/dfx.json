{
  "canisters": {
    "dotmeet_backend": {
      "build": "npx azle dotmeet_backend",
      "candid": "src/dotmeet_backend/dotmeet_backend.did",
      "gzip": true,
      "main": "src/dotmeet_backend/src/index.ts",
      "type": "custom",
      "wasm": ".azle/dotmeet_backend/dotmeet_backend.wasm"
    },
    "dotmeet_telegram": {
      "build": "npx azle dotmeet_telegram",
      "candid": "src/dotmeet_telegram/dotmeet_telegram.did",
      "gzip": true,
      "main": "src/dotmeet_telegram/src/index.ts",
      "type": "custom",
      "wasm": ".azle/dotmeet_telegram/dotmeet_telegram.wasm"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
